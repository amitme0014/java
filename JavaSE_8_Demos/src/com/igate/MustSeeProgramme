package com.amit.in;

public class MustSeeProgramme {

	private int employeeId;
	private String firstName;
	private String lastName;
	
	
	public MustSeeProgramme(int employeeId, String firstName, String lastName) {
		super();
		this.employeeId = employeeId;
		this.firstName = firstName;
		this.lastName = lastName;
	}
	public int getEmployeeId() {
		return employeeId;
	}
	public void setEmployeeId(int employeeId) {
		this.employeeId = employeeId;
	}
	public String getFirstName() {
		return firstName;
	}
	public void setFirstName(String firstName) {
		this.firstName = firstName;
	}
	public String getLastName() {
		return lastName;
	}
	public void setLastName(String lastName) {
		this.lastName = lastName;
	}
	@Override
	public String toString() {
		return "TipsForEcllipse [employeeId=" + employeeId + ", firstName=" + firstName + ", lastName=" + lastName
				+ "]";
	}
	
	/*Now using an object from above class*/
	
	public static void main(String[] args){
		
		MustSeeProgramme object=new MustSeeProgramme(12, "Amit", "Singh");
				object.getEmployeeId();
				object.getFirstName();
				object.getLastName();
				object.toString();
				
				System.out.println(object.getEmployeeId());
				System.out.println(object.toString());
		
		
	}
}
